<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="background" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="20.0" stylesheets="@java.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemospring2024.PrimaryController">
   <children>
      <Label fx:id="label" stylesheets="@java.css" text="Register">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Label fx:id="label" stylesheets="@java.css" text="Create New Account">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="createEmailTextField" promptText="Username">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="createPassTextField" promptText="Password">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#registerButtonClicked" text="Register" />
   </children>
</VBox>
